{
"name": "<%= mode==='full' ? projectName + '-server' : projectName %>",
    "version": "1.0.0",
    "description": "Express API server",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"<% if (orm==='prisma' ) { %>,
        "prisma:generate": "prisma generate",
        "prisma:push": "prisma db push"<% } %>
            },
            "dependencies": {
            "express": "^4.21.2",
            "cors": "^2.8.5",
            "helmet": "^8.0.0",
            "dotenv": "^16.4.7",
            "morgan": "^1.10.0",
            "zod": "^3.24.1"<% if (auth !=='none' ) { %>,
                "bcryptjs": "^2.4.3"<% } %>
                    <% if (auth==='jwt' || auth==='passport' ) { %>,
                        "jsonwebtoken": "^9.0.2"<% } %>
                            <% if (auth==='session' || auth==='passport' ) { %>,
                                "express-session": "^1.18.0"<% if (database==='mongodb' ) { %>,
                                    "connect-mongo": "^5.1.0"<% } else if (database==='postgresql' ) { %>,
                                        "connect-pg-simple": "^9.0.1"<% } %>
                                            <% } %>
                                                <% if (auth==='passport' ) { %>,
                                                    "passport": "^0.7.0",
                                                    "passport-local": "^1.0.0",
                                                    "passport-google-oauth20": "^2.0.0",
                                                    "passport-github2": "^0.1.12"<% } %>
                                                        <% if (database==='mongodb' ) { %>,
                                                            "mongoose": "^8.8.4"<% } else if (database==='postgresql' )
                                                                { %>
                                                                <% if (orm==='prisma' ) { %>,
                                                                    "@prisma/client": "^6.1.0"<% } else { %>,
                                                                        "pg": "^8.13.1"<% } %>
                                                                            <% } %>
                                                                                <% if (payment==='stripe' ) { %>,
                                                                                    "stripe": "^17.4.0"<% } else if
                                                                                        (payment==='paystack' ) { %>,
                                                                                        "axios": "^1.7.9"<% } %>
                                                                                            },
                                                                                            "devDependencies": {
                                                                                            "nodemon": "^3.1.9",
                                                                                            "typescript": "^5.7.2",
                                                                                            "@types/node": "^22.10.2",
                                                                                            "@types/express": "^5.0.0",
                                                                                            "@types/cors": "^2.8.17",
                                                                                            "@types/morgan": "^1.9.9",
                                                                                            "ts-node": "^10.9.2"<% if
                                                                                                (auth !=='none' ) { %>,
                                                                                                "@types/bcryptjs":
                                                                                                "^2.4.6"<% } %>
                                                                                                    <% if (auth==='jwt'
                                                                                                        ||
                                                                                                        auth==='passport'
                                                                                                        ) { %>,
                                                                                                        "@types/jsonwebtoken":
                                                                                                        "^9.0.6"<% } %>
                                                                                                            <% if
                                                                                                                (auth==='session'
                                                                                                                ||
                                                                                                                auth==='passport'
                                                                                                                ) { %>,
                                                                                                                "@types/express-session":
                                                                                                                "^1.18.0"
                                                                                                                <% if
                                                                                                                    (database==='postgresql'
                                                                                                                    ) {
                                                                                                                    %>,
                                                                                                                    "@types/connect-pg-simple":
                                                                                                                    "^7.0.3"
                                                                                                                    <% }
                                                                                                                        %>
                                                                                                                        <% }
                                                                                                                            %>
                                                                                                                            <% if
                                                                                                                                (auth==='passport'
                                                                                                                                )
                                                                                                                                {
                                                                                                                                %>
                                                                                                                                ,
                                                                                                                                "@types/passport":
                                                                                                                                "^1.0.16",
                                                                                                                                "@types/passport-local":
                                                                                                                                "^1.0.38",
                                                                                                                                "@types/passport-google-oauth20":
                                                                                                                                "^2.0.16",
                                                                                                                                "@types/passport-github2":
                                                                                                                                "^1.2.9"
                                                                                                                                <% }
                                                                                                                                    %>
                                                                                                                                    <% if
                                                                                                                                        (database==='postgresql'
                                                                                                                                        &&
                                                                                                                                        orm
                                                                                                                                        !=='prisma'
                                                                                                                                        )
                                                                                                                                        {
                                                                                                                                        %>
                                                                                                                                        ,
                                                                                                                                        "@types/pg":
                                                                                                                                        "^8.11.10"
                                                                                                                                        <% }
                                                                                                                                            %>
                                                                                                                                            <% if
                                                                                                                                                (database==='postgresql'
                                                                                                                                                &&
                                                                                                                                                orm==='prisma'
                                                                                                                                                )
                                                                                                                                                {
                                                                                                                                                %>
                                                                                                                                                ,
                                                                                                                                                "prisma":
                                                                                                                                                "^6.1.0"
                                                                                                                                                <% }
                                                                                                                                                    %>
                                                                                                                                                    }
                                                                                                                                                    }