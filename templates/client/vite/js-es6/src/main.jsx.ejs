import React from 'react';
import ReactDOM from 'react-dom/client';
<%_ if (state==='redux' ) { _%>
import { Provider } from 'react-redux';
import { store } from './store/index.js';
<%_ } _%>
import App from './App';
import './index.css';

ReactDOM.createRoot(document.getElementById('root')).render(
<React.StrictMode>
    <%_ if (state==='redux' ) { _%>
    <Provider store={store}>
        <App />
    </Provider>
    <%_ } else { _%>
    <App />
    <%_ } _%>
</React.StrictMode>
);